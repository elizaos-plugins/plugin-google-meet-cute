# Google Account for Meet Access
# The email used to join meetings
GOOGLE_MEET_EMAIL=your-bot@gmail.com
# Optional: Only needed if not using OAuth
GOOGLE_MEET_PASSWORD=your-password

# Optional: Meeting defaults
DEFAULT_MEETING_NAME="ElizaOS Bot"
TRANSCRIPTION_LANGUAGE=en
AUDIO_CHUNK_DURATION_MS=30000

# Optional: Report generation settings
REPORT_OUTPUT_DIR=./meeting-reports
ENABLE_REAL_TIME_TRANSCRIPTION=true

# Note: This plugin uses OpenAI Whisper for transcription
# Make sure OPENAI_API_KEY is set in your main .env file 